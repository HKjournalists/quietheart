void clickedSlot()
{
	//在处理按钮压下的槽里面获得发送clicked信号的按钮对象。
QPushButton *sendButton = qobject_cast<QPushButton *>(sender());
setRotation(sendButton->objectName().remove("rot").toInt());

}

