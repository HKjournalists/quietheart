#: ../libwnck/wnckprop.c:279
msgid "Change the name of the workspace to NAME"
msgstr "Змінити назву робочої області на НАЗВА"

#: ../libwnck/wnckprop.c:279
msgid "NAME"
msgstr "НАЗВА"

#: ../libwnck/wnckprop.c:281
